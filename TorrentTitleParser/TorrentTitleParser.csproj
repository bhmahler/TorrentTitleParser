<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0;netstandard2.0;netstandard2.1;</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Brian Mahler</Authors>
    <Description>Library for parsing movie and tv info from torrent names</Description>
    <Copyright />
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/bhmahler/TorrentTitleParser</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <Version>1.0.5</Version>
    <AssemblyVersion>1.0.5.0</AssemblyVersion>
    <PackageReleaseNotes>
		• Added support for .NET 7 and .NET Stnadard 2.0
		• Fixed issue with year parsing for years starting with 19</PackageReleaseNotes>
    <FileVersion>1.0.5.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
