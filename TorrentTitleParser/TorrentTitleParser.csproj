<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Brian Mahler</Authors>
    <Description>Library for parsing movie and tv info from torrent names</Description>
    <Copyright />
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/bhmahler/TorrentTitleParser</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <Version>1.0.3</Version>
    <AssemblyVersion>1.0.3.0</AssemblyVersion>
    <PackageReleaseNotes>• Added Audio Channels to parsed properties
• Added Dolby Atmos to parsed properties
• Added TrueHD to parsed properties
• Added additional tests for alternative torrent formats
• Improved various matching patterns
• Updated pattern attribute to support text replacements within match
• Updated audio codec parsing and removed flags and channels from codec
• Updated existing tests for added parsed properties</PackageReleaseNotes>
    <FileVersion>1.0.3.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
